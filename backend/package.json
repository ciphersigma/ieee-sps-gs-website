{
  "name": "ieee-sps-backend",
  "version": "1.0.0",
  "description": "Backend API for IEEE SPS Gujarat website",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "prod": "cross-env NODE_ENV=production node server.js",
    "init-data": "node scripts/initData.js",
    "init-all": "node scripts/initAllData.js",
    "init-users": "node scripts/initUsers.js",
    "add-user": "node scripts/addUser.js",
    "clean": "del debug-events.js test-create-event.js test-upload.js",
    "install-legacy": "npm install --legacy-peer-deps"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
